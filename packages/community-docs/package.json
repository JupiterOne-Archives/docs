{
  "name": "@jupiterone/docs-community",
  "version": "1.0.0",
  "main": "index.ts",
  "repository": "git@github.com:JupiterOne/docs-community.git",
  "author": "Bryanschauerte <bryan.schauerte@gmail.com>",
  "license": "NONE",
  "private": true,
  "scripts": {
    "run": "yarn ts-node ./src/index.ts",
    "watch-and-run-fake-httpClient": "ts-node-dev --respawn -- src/index.ts testing",
    "watch-and-run": "ts-node-dev --respawn -- src/index.ts",
    "test": "jest",
    "clean": "rm -rf ./dist ./coverage",
    "compile": "ttsc -p tsconfig.dist.json; cp package.json ./dist; cd ./dist;",
    "autobuild": "yarn compile --watch",
    "build": "yarn clean && yarn compile",
    "server": "./tools/bin/run-service.sh",
    "start": "concurrently 'yarn autobuild' 'yarn watch-and-run'"
  },
  "dependencies": {
    "@types/node": "^16.11.7",
    "axios": "^0.24.0",
    "form-data": "^4.0.0",
    "lodash": "^4.17.21",
    "rxjs": "^7.4.0",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "@jupiterone/dev-tools": "^8.2.0",
    "@jupiterone/platform-sdk-logging": "^2.1.1",
    "@jupiterone/platform-sdk-test-tools": "^1.1.0",
    "@jupiterone/typescript-tools": "^15.0.0",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.176",
    "@types/supertest": "^2.0.11",
    "concurrently": "^6.4.0",
    "dotenv": "^10.0.0",
    "simple-git": "^2.47.0",
    "supertest": "^6.1.6",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8"
  }
